{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net9.0/TimeSheetAPI.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "launchSettingsFilePath": "${workspaceFolder}/Properties/launchSettings.json",
      "launchBrowser": {
        "enabled": true,
        "windows": {
          "command": "cmd.exe",
          "args": "/C start ${auto-detect-url}/swagger/index.html"
        },
        "osx": {
          "command": "open",
          "args": "${auto-detect-url}/swagger/index.html"
        }
      }
    }
    //,
    // {
    //   "name": "C#: TimeSheetAPI [Default Configuration]",
    //   "type": "dotnet",
    //   "request": "launch",
    //   "projectPath": "${workspaceFolder}\\TimeSheetAPI.csproj"
    // }
  ]
}
